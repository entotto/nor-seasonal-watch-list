{
    "name": "unheppcat/nor-seasonal-watch-list",
    "description": "NOR Seasonal Watch List",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "babdev/pagerfanta-bundle": "^3.2",
        "composer/package-versions-deprecated": "1.11.99.1",
        "demontpx/parsedown-bundle": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "erusev/parsedown-extra": "^0.8.1",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "guzzlehttp/guzzle": "7.2 as 6.3",
        "knpuniversity/oauth2-client-bundle": "^2.7",
        "mockery/mockery": "^1.4",
        "nelmio/cors-bundle": "^2.1",
        "pagerfanta/doctrine-dbal-adapter": "^3.1",
        "pagerfanta/doctrine-orm-adapter": "^3.1",
        "pagerfanta/twig": "^3.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpunit/phpunit": "^9.5",
        "psr/log": "^2.0",
        "restcord/restcord": "dev-develop as 0.5.x-dev",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.4.*",
        "symfony/browser-kit": "^5.2",
        "symfony/console": "5.4.*",
        "symfony/css-selector": "^5.2",
        "symfony/debug-bundle": "^5.2",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/maker-bundle": "^1.28",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.4.*",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/stopwatch": "^5.2",
        "symfony/string": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "5.4.*",
        "symfony/var-dumper": "^5.2",
        "symfony/web-link": "5.4.*",
        "symfony/web-profiler-bundle": "^5.2",
        "symfony/yaml": "5.4.*",
        "twbs/bootstrap": "^5.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "wohali/oauth2-discord-new": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json":  "symfony-cmd"
        },
        "post-install-cmd": [
            "npm install --force",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "npm install --force",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
